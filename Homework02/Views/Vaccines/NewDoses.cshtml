@model List<Vaccine> 
<form method="post" id="adddoses">

    <div class="form-group">
        <label for="id">Vaccine</label>
        <select name="id" id="id" form="adddoses">
            @foreach (var vaccine in Model)
            {
                <option value=@vaccine.Id>@vaccine.Name</option>
            }
        </select>
    </div>

    <div class="form-group">
        <label for="doses">New Doses Recieved</label>
        <input type="number" class="form-control" id="doses" name="doses" required/>
    </div>

    <button type="submit" class="btn btn-primary">Add</button>
</form>